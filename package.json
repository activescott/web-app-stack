{
  "name": "activescott-web-app-stack",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run -s build-react && npm run -s build-server",
    "build-react": "pushd . && cd ./src/react-app && npm run build && popd",
    "build-server": "npx tsc",
    "start": "npm run -s build && npx arc sandbox",
    "deploy": "npm run build && npx arc deploy",
    "eslint": "./node_modules/.bin/eslint . --ext ts,tsx",
    "lint": "./node_modules/.bin/prettier -l \"{,!(node_modules)/**/}*.{ts,tsx,md,yml,json,html}\" && npm run eslint",
    "lint-fix": "./node_modules/.bin/prettier --write \"{,!(node_modules)/**/}*.{ts,tsx,md,yml,json,html}\" && npm run eslint --fix",
    "install-all": "pushd . && npm install && cd ./src/react-app && npm install && popd"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@activescott/eslint-config": "^2.2.1",
    "@architect/architect": "^6.5.2",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "@architect/functions": "^3.12.1",
    "typescript": "^3.9.5"
  }
}
